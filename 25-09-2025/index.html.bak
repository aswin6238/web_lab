<html>
<frameset rows="30%, 70%">
 <!-- Top horizontal frame -->
 <frame src="top_frame.html" name="topFrame" noresize>
 <!-- Bottom frame, further divided vertically -->
 <frameset cols="25%, 50%, 25%">
 <!-- Left vertical frame -->
 <frame src="left_frame.html" name="leftFrame">
 <!-- Middle vertical frame -->
 <frame src="middle_frame.html" name="middleFrame">
 <!-- Right vertical frame -->
 <frame src="right_frame.html" name="rightFrame">
 </frameset>
 <noframes>
 <body>
 Your browser does not support frames.
 </body>
 </noframes>
</frameset>
</html>
